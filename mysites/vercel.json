{
  "build": {
    "env": {
      "PIP_REQUIREMENTS": "requirements.txt"
    }
  },
  "builds": [
    {
      "src": "api/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/wsgi.py"
    }
  ],
  "outputDirectory": "staticfiles"
}





