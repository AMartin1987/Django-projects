{
  "routes": [
    {
      "src": "/blog/static/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/blog/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "manage.py"
    }
  ],
  "builds": [
    { "src": "manage.py", "use": "@vercel/python" }
  ]
}

