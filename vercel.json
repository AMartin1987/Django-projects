{
  "version": 2,
  "builds": [
    {
      "src": "mysites/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/blog/static/(.*)",
      "dest": "/blog/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "mysites/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "mysites.settings",
    "PYTHONPATH": "mysites"
  }
}
